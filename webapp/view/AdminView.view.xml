<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout"
	controllerName="TacticTool.controller.AdminView">
	<App id="app">
		<pages>
			<Page id="page" title="Tactic Tool" class="sapUiContentPadding">
				<customHeader title="Tactic Tool">
					<Toolbar>
						<content>
							<FlexBox width="100%">
								<items>
									<Button text="Back to List" press="navBack" id="backToDetailButton" icon="sap-icon://nav-back"/>
								</items>
							</FlexBox>
						</content>
					</Toolbar>
				</customHeader>
				<content>
					<VBox width="100%" direction="Column">
						<items>
							<Text text="Combobox Items" width="100%" class="sapUiSmallMargin"/>
						</items>
					</VBox>
				</content>
				<content>
					<layout:Grid defaultSpan="XL6 L6 M6 S6">
						<layout:content>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text xmlns="sap.m" text="Priority"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" alignItems="Inherit">
											<items>
												<Button xmlns="sap.m" id="addPriority" press="addPriorityBtn" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Priority {Item}" id="priorityItemID"/>
								</List>
							</VBox>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text text="Status"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" alignItems="Inherit">
											<items>
												<Button id="addStatus" press="addStatusBtn" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Status {Item}" id="statusItemID"/>
								</List>
							</VBox>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text text="Subcluster"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" id="bbox0_copy5" alignItems="Inherit">
											<items>
												<Button id="addSubcluster" press="addSubclusterBtn" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Subcluster {Item}" id="subclusterItemID"/>
								</List>
							</VBox>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text text="Campaign Type"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" alignItems="Inherit">
											<items>
												<Button id="addCampaignType" press="addCampaignTypeBtn" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Campaign Type {Item}" id="campaignTypeItemID"/>
								</List>
							</VBox>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text text="Specified Campaign Type"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" alignItems="Inherit">
											<items>
												<Button id="addSpecifiedCampaignType" press="addSpecifiedCampaignTypeBtn" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Specified Campaign Type {Item}" id="specifiedCampaignTypeItemID"/>
								</List>
							</VBox>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text text="Marketeer"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" alignItems="Inherit">
											<items>
												<Button id="addMarketeer" press="addMarketeerBtn" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Marketeer {Item}" id="marketeerItemID"/>
								</List>
							</VBox>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text text="Host Venue"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" alignItems="Inherit">
											<items>
												<Button id="addHostVenue" press="addHostVenue" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Host Venue {Item}" id="hostVenueItemID"/>
								</List>
							</VBox>
							<VBox width="100%" direction="Column">
								<HBox width="100%">
									<items>
										<FlexBox width="100%" alignContent="SpaceAround" alignItems="Center">
											<items>
												<Text text="Type of Costs"/>
											</items>
										</FlexBox>
										<FlexBox width="100%" alignItems="Inherit">
											<items>
												<Button id="addTypeOfCosts" press="addTypeOfCostsBtn" icon="sap-icon://add" type="Transparent" text="Add Item"/>
											</items>
										</FlexBox>
									</items>
								</HBox>
								<List mode="Delete" delete="handleDelete">
									<InputListItem label="Type of Costs {Item}" id="typeOfCostsItemID"/>
								</List>
							</VBox>
						</layout:content>
					</layout:Grid>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>